{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"no-console": "off",
		"no-constant-condition": "off",
		"indent": "off",
		"no-unused-vars": "off",
		"no-empty-pattern": "off",
		"quotes": [
			"error",
			"double"
		],
		"linebreak-style": [
			"warn",
			"unix"
		],
		"semi": [
			"error",
			"always"
		]
	}
}